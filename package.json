{
  "name": "api-lista-compras-node",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "ts-node-dev src/server.ts --ignore _tests_",
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules src/server.ts --ignore _tests_",
    "typeorm": "ts-node-dev ./node_modules/typeorm/cli.js",
    "knex:migration": "knex --knexfile knexfile.ts migrate:latest --env development",
    "pretest": "knex --knexfile knexfile.ts migrate:latest --env test",
    "test": "jest",
    "posttest": "knex --knexfile knexfile.ts migrate:rollback --all --env test"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jest": "^26.6.3",
    "knex": "^0.21.12",
    "sqlite3": "^5.0.0",
    "supertest": "^6.0.1",
    "typeorm": "^0.2.29"
  },
  "devDependencies": {
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.15",
    "@types/knex": "^0.16.1",
    "@types/supertest": "^2.0.10",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.1.2"
  }
}